{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",

  "GithubService": {
    "ClientId": null,
    "ClientSecret": null,
    "GetOAuth2CodeUrl": "https://github.com/login/oauth/authorize",
    "GetTokenUrl": "https://github.com/login/oauth/access_token",
    "GetReposUrl": "https://api.github.com/user/repo"
  },

  "GithubSession": {
    "StateKey": "GithubState",
    "AccessTokenKey": "GithubAccessToken"
  },

  "GoogleService": {
    "ClientId": null,
    "ClientSecret": null,
    "GetOAuth2CodeUrl": "https://accounts.google.com/o/oauth2/v2/auth",
    "GetTokensUrl": "https://www.googleapis.com/oauth2/v4/token",
    "GetUserInfoUrl": "https://www.googleapis.com/oauth2/v3/userinfo",
    "StateSessionKey": "GoogleStateSessionKey"
  },

  "GoogleSession": {
    "StateKey": "GoogleState",
    "AccessTokenKey": "GoogleAccessToken",
    "IdTokenKey": "GoogleIdToken"
  }
}
